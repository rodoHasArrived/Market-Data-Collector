{
  "Application": {
    "HttpPort": 8080,
    "LogLevel": "Information"
  },
  "DataPath": "./data",
  "Providers": [
    {
      "Name": "alpaca",
      "Type": "PythonSubprocess",
      "Enabled": true,
      "ScriptPath": "./plugins/alpaca_collector.py",
      "Symbols": ["SPY", "QQQ", "AAPL", "MSFT"]
    },
    {
      "Name": "polygon",
      "Type": "PythonSubprocess",
      "Enabled": false,
      "ScriptPath": "./plugins/polygon_collector.py",
      "Symbols": ["SPY", "QQQ"]
    },
    {
      "Name": "ib",
      "Type": "InteractiveBrokers",
      "Enabled": false,
      "Symbols": [
        {
          "Symbol": "SPY",
          "SecurityType": "STK",
          "Exchange": "SMART",
          "Currency": "USD"
        },
        {
          "Symbol": "ES",
          "SecurityType": "FUT",
          "Exchange": "CME",
          "Currency": "USD"
        }
      ]
    }
  ],
  "Storage": {
    "Type": "SQLite",
    "Path": "./data/market_data.db"
  }
}
