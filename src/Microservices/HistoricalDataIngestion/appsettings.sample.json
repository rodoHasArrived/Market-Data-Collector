{
  "HistoricalService": {
    "ServiceName": "DataIngestion.HistoricalService",
    "HttpPort": 5004,
    "MessageBus": {
      "Transport": "InMemory",
      "RabbitMq": { "Host": "localhost", "Port": 5672, "Username": "guest", "Password": "guest" }
    },
    "Storage": { "DataDirectory": "data/historical" },
    "Backfill": {
      "MaxConcurrentJobs": 5,
      "BatchSize": 1000,
      "RetryCount": 3,
      "RetryDelayMs": 1000,
      "ProgressReportIntervalSeconds": 10
    },
    "Providers": {
      "Yahoo": { "Enabled": true, "Priority": 1 },
      "Stooq": { "Enabled": true, "Priority": 2 }
    }
  },
  "Serilog": {
    "MinimumLevel": { "Default": "Information", "Override": { "Microsoft": "Warning" } },
    "WriteTo": [{ "Name": "Console" }]
  }
}
